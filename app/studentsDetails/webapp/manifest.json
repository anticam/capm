{
    "_version": "1.8.0",
    "sap.app": {
        "id": "student",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "type": "application",
        "title": "Students Details",
        "description": "studentsDetails",
        "dataSources": {
            "mysrvdemoapp": {
                "uri": "/odata/v4/mysrvdemoapp/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.115.1",
            "libs": {
                "sap.fe.templates": {}
            }
        },
        "models": {
            "": {
                "dataSource": "mysrvdemoapp",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": false,
                    "earlyRequests": false,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "Student",
                    "target": "Student"
                },
                {
                    "pattern": "GetStudent({key}):?query:",
                    "name": "StudentDetail",
                    "target": "StudentDetail"
                }
            ],
            "targets": {
                "Student": {
                    "type": "Component",
                    "id": "Student",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "GetStudent",
                            "navigation": {
                                "GetStudent": {
                                    "detail": {
                                        "route": "StudentDetail"
                                    }
                                }
                            }
                        }
                    }
                },
                "StudentDetail": {
                    "type": "Component",
                    "id": "StudentDetail",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "GetStudent"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    }
}